<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.SelectMatchCodeController">
   <children>
      <Label layoutX="29.0" layoutY="200.0" prefHeight="36.0" prefWidth="193.0" text="Enter Match Code:" textFill="#cd1010">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <TextField fx:id="matchCode_value" layoutX="235.0" layoutY="200.0" prefHeight="36.0" prefWidth="176.0" />
      <Button fx:id="matchCode" layoutX="260.0" layoutY="133.0" mnemonicParsing="false" onAction="#matchCodeAction" prefHeight="45.0" prefWidth="152.0" text="See Match Code" textFill="#62c38c">
         <font>
            <Font size="17.0" />
         </font>
      </Button>
      <Button fx:id="enter" layoutX="343.0" layoutY="254.0" mnemonicParsing="false" onAction="#enterAction" prefHeight="36.0" prefWidth="68.0" text="Enter" textFill="#62c38c">
         <font>
            <Font size="17.0" />
         </font>
      </Button>
      <Button fx:id="reset" layoutX="255.0" layoutY="254.0" mnemonicParsing="false" onAction="#resetAction" prefHeight="36.0" prefWidth="68.0" text="Reset" textFill="#62c38c">
         <font>
            <Font size="17.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
